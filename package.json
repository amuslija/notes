    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "firebase": "^4.1.3",
    "node-sass-chokidar": "0.0.3",
    "node-uuid": "^1.4.8",
    "npm-run-all": "^4.0.2",
